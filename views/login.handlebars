<form class="login-form">
  <div>
    <label for="username-login">Username:</label>
    <input type="text" id="username-login" class="form-input"/>
  </div>
  <div>
    <label for="password-login">Password:</label>
    <input type="password" id="password-login" class="form-input"/>
  </div>
  <div>
    <button type="submit">Login</button>
  </div>
  <div>
    <a href="/sign-up" class="text-decoration-none">Click to Sign Up</a>
  </div>
</form>
<script src="/javascript/login.js"></script>
